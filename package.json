{
   "name": "auth-service",
   "version": "1.0.0",
   "description": "The user management service",
   "type": "commonjs",
   "main": "index.js",
   "scripts": {
      "start": "ts-node src/index.ts",
      "build": "tsc",
      "dev": "cross-env NODE_ENV= dev nodemon --exec ts-node src/server.ts",
      "format:fix": "prettier . --write",
      "format:check": "prettier . --check",
      "lint:check": "eslint .",
      "lint:fix": "eslint . --fix",
      "test": "jest --watch --runInBand",
      "prepare": "husky",
      "typeorm": "typeorm-ts-node-commonjs"
   },
   "author": "vaishnavi",
   "license": "ISC",
   "devDependencies": {
      "@eslint/js": "^9.21.0",
      "@types/eslint": "^9.6.1",
      "@types/express": "^5.0.0",
      "@types/http-errors": "^2.0.4",
      "@types/jest": "^29.5.14",
      "@types/node": "^16.11.10",
      "@types/supertest": "^6.0.2",
      "@types/winston": "^2.4.4",
      "@typescript-eslint/eslint-plugin": "^8.25.0",
      "@typescript-eslint/parser": "^8.25.0",
      "cross-env": "^7.0.3",
      "eslint": "^9.21.0",
      "eslint-plugin-jsx-a11y": "^6.10.2",
      "eslint-plugin-react": "^7.37.4",
      "eslint-plugin-react-hooks": "^5.1.0",
      "globals": "^16.0.0",
      "husky": "^9.1.7",
      "jest": "^29.7.0",
      "lint-staged": "^15.4.3",
      "nodemon": "^3.1.9",
      "prettier": "3.5.2",
      "supertest": "^7.0.0",
      "ts-jest": "^29.2.6",
      "ts-node": "10.9.1",
      "typescript": "4.5.2",
      "typescript-eslint": "^8.24.1"
   },
   "dependencies": {
      "debug": "^4.4.0",
      "dotenv": "^16.4.7",
      "express": "^4.21.2",
      "http-errors": "^2.0.0",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.21",
      "winston": "^3.17.0"
   },
   "lint-staged": {
      "*.ts": [
         "npm run format:fix",
         "npm run lint:fix"
      ]
   }
